{
  "$schema": "https://schemas.examplary.ai/question-type.json",

  "id": "examplary.default.multi-line-text",
  "public": true,

  "name": {
    "en": "Long answer",
    "nl": "Uitgebreid antwoord"
  },
  "description": {
    "en": "A question that requires a multi-line text response.",
    "nl": "Een vraag met een lang antwoord."
  },

  "icon": "./icon.svg",
  "components": {
    "assessment": "./component-assessment.tsx",
    "print": "./component-print.tsx"
  },

  "settings": [],

  "export": {
    "qti3": {
      "interaction": {
        "type": "ExtendedTextInteraction",
        "options": {}
      }
    }
  },

 "translations": {
    "placeholder": {
      "en": "Type your answer...",
      "nl": "Typ je antwoord..."
    }
  },

  "generation": {
    "enabled": true
  },
  "grading": {
    "enabled": true
  },
  "timeEstimateMinutes": 5,

  "shortcut": "2",
  "index": 101
}
